{
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "arrowParens": "always",
        "bracketSpacing": true,
        "endOfLine": "crlf",
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": false,
        "printWidth": 100,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": false,
        "tabWidth": 2,
        "trailingComma": "none",
        "useTabs": false,
        "vueIndentScriptAndStyle": false,
        "parser": "babel-ts"
      }
    }
  ]
}
